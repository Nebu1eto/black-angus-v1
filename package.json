{
  "name": "black-angus-bot",
  "version": "1.1.0",
  "description": "Multi-Purpose Discord Bot",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "ava",
    "build": "tsc",
    "lint": "eslint --fix --ext .js,.ts .",
    "clean": "rimraf package-lock.json node-modules && npm install"
  },
  "author": "Haze Lee <hazelee@realignist.me>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/bluebird": "^3.5.31",
    "@types/charset": "^1.0.1",
    "@types/cheerio": "^0.22.18",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.151",
    "@types/mongoose": "^5.7.20",
    "@types/node": "^12",
    "@types/sharp": "^0.25.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "ava": "^3.8.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^17.0.0",
    "eslint-plugin-ava": "^10.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.7",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.1.0",
    "apng2gif-bin": "^1.7.1",
    "bluebird": "^3.7.2",
    "bufferutil": "^4.0.1",
    "charset": "^1.0.1",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.13.0",
    "debug": "^4.1.1",
    "discord.js": "^12.2.0",
    "fuzzball": "^1.3.0",
    "got": "^11.1.4",
    "iconv-lite": "^0.5.1",
    "ioredis": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.14",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "sharp": "^0.25.2"
  },
  "engines": {
    "node": "=12.x.x",
    "npm": "=6.x.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
